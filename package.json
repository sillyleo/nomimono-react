{
	"name": "nomi",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"ship": "turbo run ship",
		"storybook": "turbo run storybook",
		"build-token": "turbo run build-token"
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "latest",
		"react-docgen": "^5.4.3",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.29.1"
}
