{
	"name": "nomimono",
	"version": "0.0.304",
	"access": "public",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"license": "MIT",
	"author": "Leo Lin",
	"bugs": {
		"url": "https://github.com/sillyleo/nomimono/issues"
	},
	"homepage": "https://github.com/sillyleo/nomimono#readme",
	"scripts": {
		"lint": "eslint *.ts*",
		"build": "tsup index.tsx --format esm,cjs --dts --external react",
		"dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
		"ship": "tsup index.tsx --format esm,cjs --dts --external react && npm version patch && npm publish --access public",
		"build-token": "npx token-transformer ./tokens/figma-output/ ./tokens/theme-transformed.json --resolveReferences false && npx style-dictionary build --config sd.config.cjs",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@changesets/cli": "^2.26.0",
		"@macaron-elements/loader-webpack": "^0.3.1",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-docs": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack4": "^6.5.16",
		"@storybook/cli": "^6.5.16",
		"@storybook/manager-webpack4": "^6.5.16",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"@types/lodash": "^4.14.191",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"babel-loader": "^8.3.0",
		"eslint": "^7.32.0",
		"eslint-config-custom": "workspace:*",
		"react": "^18.2.0",
		"tsconfig": "workspace:*",
		"tsup": "^6.6.3",
		"typescript": "^4.5.2"
	},
	"dependencies": {
		"@radix-ui/colors": "^0.1.8",
		"@stitches/react": "^1.2.8",
		"framer-motion": "^10.2.4",
		"lodash": "^4.17.21"
	}
}
