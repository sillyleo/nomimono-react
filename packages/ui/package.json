{
    "name": "nomimono",
    "version": "0.0.304",
    "access": "public",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "license": "MIT",
    "author": "Leo Lin",
    "bugs": {
        "url": "https://github.com/sillyleo/nomimono/issues"
    },
    "homepage": "https://github.com/sillyleo/nomimono#readme",
    "scripts": {
        "lint": "eslint *.ts*",
        "build": "tsup index.tsx --format esm,cjs --dts --external react",
        "watch": "tsup index.tsx --format esm,cjs --watch --dts --external react",
        "ship": "tsup index.tsx --format esm,cjs --dts --external react && npm version patch && npm publish --access public",
        "build-token": "npx token-transformer ./tokens/figma-output/ ./tokens/theme-transformed.json --resolveReferences false && npx style-dictionary build --config sd.config.cjs",
        "storybook": "storybook dev -p 4000",
        "dev": "turbo run watch & turbo run storybook",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.0",
        "@changesets/cli": "^2.26.0",
        "@macaron-elements/loader-webpack": "^0.3.1",
        "@storybook/addon-actions": "^7.0.0-rc.3",
        "@storybook/addon-docs": "^7.0.0-rc.3",
        "@storybook/addon-essentials": "^7.0.0-rc.3",
        "@storybook/addon-interactions": "^7.0.0-rc.3",
        "@storybook/addon-links": "^7.0.0-rc.3",
        "@storybook/addon-mdx-gfm": "^7.0.0-rc.3",
        "@storybook/blocks": "^7.0.0-rc.3",
        "@storybook/cli": "^7.0.0-rc.3",
        "@storybook/react": "^7.0.0-rc.3",
        "@storybook/react-webpack5": "^7.0.0-rc.3",
        "@storybook/testing-library": "^0.0.14-next.1",
        "@types/lodash": "^4.14.191",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "babel-loader": "^8.3.0",
        "eslint": "^7.32.0",
        "eslint-config-custom": "workspace:*",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "18.2.0",
        "storybook": "^7.0.0-rc.3",
        "tsconfig": "workspace:*",
        "tsup": "^6.6.3",
        "typescript": "^4.5.2"
    },
    "dependencies": {
        "@radix-ui/colors": "^0.1.8",
        "@stitches/react": "^1.2.8",
        "framer-motion": "^10.2.4",
        "lodash": "^4.17.21"
    }
}
